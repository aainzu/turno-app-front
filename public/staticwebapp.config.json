{
  "routes": [
    {
      "route": "*",
      "methods": ["POST", "PUT", "DELETE"],
      "rewrite": "/api/render"
    },
    {
      "route": "/",
      "rewrite": "/api/render"
    },
    {
      "route": "/index.html",
      "rewrite": "/api/render"
    }
  ],
  "navigationFallback": {
    "rewrite": "/api/render"
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=3600"
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".css": "text/css",
    ".json": "application/json"
  }
}
